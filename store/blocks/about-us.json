{
  "store.custom#about-us": {
    "blocks": [
      "flex-layout.row#about-us",
      "list-context.image-list#demo0",
      "geolocation"
      
    ]
  },

  "flex-layout.row#about-us": {
    "children": [
      "image#mobile-phone",
      
      "flex-layout.col#text-about-us"
    ]
  },

  "flex-layout.col#text-about-us": {
    "children": [
      "rich-text#title-about-us",
      "rich-text#about-us",
      "rich-text#title-about-uss",
      "rich-text#about-uss"
      
     
    ],
    "props": { 
      
      "blockclass": "textColumn",
      "preventVerticalStretch": true
    }
  },

  "rich-text#title-about-us": {
    "props": {
      "text":
      "Unde ne gasiti?",
      "blockClass": "title"
    }
  },

  "image#mobile-phone": {
    "props": {
      "src": "https://media.giphy.com/media/3tLrOmu4C0nOE/giphy.gif",
      "width": "800px",
      "height":"400px",
      
      "blockClass": "storePrint"
    }
  },
  "video#about-us": {
    "props": {
      "src": "https://www.youtube.com/embed/JgkrlaF52WQ",
      "width": "100%",
      "height": "100%"
    }
  },
  "rich-text#about-us": {
    "props": {
      "text": "*-Strada Pierre de Coubertin nr. 3-5, lângă Arena Națională, Bucureşti, Mega Mall, etajul 1 \n -Strada Lipscani, nr. 55, Centrul Istoric, București \n -Str. Arthur Verona 13-15, Sector 1, Bucureşti*",
      "blockClass": "about"
    }
  },

  "rich-text#title-about-uss": {
    "props": {
      "text":
      "Contact:",
      "blockClass": "title"
    }
  }
}
